      gainprm="0.01568627451 0 0" biasprm="0 -100 -10"/>
  </actuator>

  <sensor>
    <framepos name="eePos" objtype="site" objname="probeEE"/>
    <framequat name="eeQuat" objtype="site" objname="probeEE"/>
    <framelinvel name="eeLinVel" objtype="site" objname="probeEE"/>
    <frameangvel name="eeAngVel" objtype="site" objname="probeEE"/>
    <gyro name="eeGyro" site="probeEE"/>
    <accelerometer name="eeAcc" site="probeEE"/>
    <force name="eeForce" site="probeEE"/>
