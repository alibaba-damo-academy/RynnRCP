                      <geom mesh="link7_2" material="white" class="visual"/>
                      <geom mesh="link7_3" material="black" class="visual"/>
                      <geom name="fr3_link7_collision" class="collision" mesh="link7_coll"/>

                      <body name="probe" pos="0 0 0.155" quat="0.9238795 0 0 -0.3826834">
                        <joint name="probe_slide" type="slide" range="0.0 0.001"/>
                        <inertial mass="0.73" pos="-0.01 0 0.03" diaginertia="0.001 0.0025 0.0017"/>
                        <geom mesh="probe_coll" material="off_white" class="visual"/>
                        <geom mesh="probe_coll" class="collision"/>

                        <site name="probeEE" pos="0.0 0 0.0" rgba="1 0 0 1"/>
                        <geom name="EE" type="sphere" size="0.005" pos="0 0 0.0" rgba="1 1 0 1" />
                        <geom type="cylinder" size="0.002 0.03" rgba="1 0 0 1" pos="0.03 0 0.035" euler="0 1.57079 0" contype="0" conaffinity="0" mass="0"/>  
                        <geom type="cylinder" size="0.002 0.03" rgba="0 1 0 1" pos="0 0.03 0.035" euler="1.57079 0 0" contype="0" conaffinity="0" mass="0"/>  
