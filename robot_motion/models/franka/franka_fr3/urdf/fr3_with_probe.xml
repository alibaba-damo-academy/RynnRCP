<mujoco model="fr3">
  <compiler angle="radian" meshdir="../assets"/>

  <default>
    <default class="fr3">
      <joint armature="0.1" damping="1"/>
      <position inheritrange="1"/>
      <default class="visual">
        <geom type="mesh" group="2" contype="0" conaffinity="0"/>
      </default>
      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>

      <default class="probe">
        <joint axis="0 0 1" type="slide" range="0 0.001"/>
      </default>
      <default class="collision">
        <geom type="mesh" group="3"/>
      </default>
    </default>
  </default>

  <contact>
    <exclude body1="fr3_link5" body2="fr3_link6"/>
    <exclude body1="fr3_link5" body2="fr3_link7"/>
    <exclude body1="fr3_link6" body2="fr3_link7"/>
    <exclude body1="fr3_link6" body2="probe"/>
    <exclude body1="fr3_link7" body2="probe"/>
    <exclude body1="fr3_link5" body2="probe"/>
  </contact>


  <asset>
